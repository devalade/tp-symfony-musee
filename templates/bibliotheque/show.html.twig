{% extends 'base.html.twig' %}

{% block title %}Bibliotheque{% endblock %}

{% block body %}
    <h1>Bibliotheque</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ bibliotheque.id }}</td>
            </tr>
            <tr>
                <th>NumMus</th>
                <td>{{ bibliotheque.numMus }}</td>
            </tr>
            <tr>
                <th>ISBN</th>
                <td>{{ bibliotheque.ISBN }}</td>
            </tr>
            <tr>
                <th>DateAchat</th>
                <td>{{ bibliotheque.dateAchat ? bibliotheque.dateAchat|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('bibliotheque_index') }}">back to list</a>

    <a href="{{ path('bibliotheque_edit', {'id': bibliotheque.id}) }}">edit</a>

    {{ include('bibliotheque/_delete_form.html.twig') }}
{% endblock %}
